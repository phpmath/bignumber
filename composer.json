{
    "name": "phpmath/bignumber",
    "description": "A PHP library to work with big numbers.",
    "keywords": [
        "bignumber",
        "math"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "PHP\\Math\\BigNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHP\\Math\\BigNumberTest\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Walter Tamboer",
            "homepage": "http://www.waltertamboer.nl",
            "role": "Developer"
        }
    ]
}
